<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pageModels="using:Examples.Maui.PageModels"
             x:Class="Examples.Maui.Pages.SampleFlyoutMenuPage"
             x:DataType="pageModels:SampleFlyoutMenuPageModel"
             Title="MENU" 
             BackgroundColor="Gray">
    <StackLayout
        Spacing="10"
        Margin="10"
    >
        <Button Text="Page1" Command="{Binding Page1Command}"/>
        <Button Text="Page2" Command="{Binding Page2Command}"/>
    </StackLayout>
</ContentPage>